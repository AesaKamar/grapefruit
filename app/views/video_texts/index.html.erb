<%= breadcrumbs([
      link_to("All Courses", courses_path),
      link_to(@course.name, course_path(@course)),
      link_to(@capsule.name, [@course, @capsule]),
      link_to(@lecture.name, [@course, @capsule, @lecture]),
      link_to('Add Video Text', void, class: "current")
    ])
%>
<div class="landing">

  <div class="description">
    <h2>Edit Video Texts</h2>
  </div>

  <div class="login">
    <%= render collection: @video_texts, partial: "edit_video_text", as: :video_text %>
  </div>

</div>

